set -g renumber-windows off
set -g mouse off

tmux_conf_new_window_retain_current_path=false
tmux_conf_new_pane_retain_current_path=true
tmux_conf_new_session_prompt=false

tmux_conf_theme_window_status_current_bg='#B27AEB'
tmux_conf_theme_window_status_fg="#B27AEB"
tmux_conf_theme_window_status_last_fg='#B27AEB'

tmux_conf_theme_status_bg='default'
tmux_conf_theme_window_status_bg="default"
tmux_conf_theme_window_status_last_bg='default'

tmux_conf_theme_window_status_format='#I'
tmux_conf_theme_window_status_current_format='#I'

tmux_conf_theme_status_left=''
tmux_conf_theme_status_right=''

tmux_conf_copy_to_os_clipboard=true